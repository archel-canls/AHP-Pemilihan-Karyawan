<%- include('partials/header') %>

<h2>Hasil Perhitungan Pemilihan Karyawan</h2>

<h3>Bobot Kriteria:</h3>
<ul>
  <% for(const kri in bobotKriteria) { %>
    <li><%= kri %>: <%= (bobotKriteria[kri]*100).toFixed(2) %> %</li>
  <% } %>
</ul>

<h3>Ranking Alternatif:</h3>
<table border="1" cellpadding="5" cellspacing="0">
  <thead>
    <tr>
      <th>Alternatif</th>
      <th>Nilai Akhir</th>
    </tr>
  </thead>
  <tbody>
    <% hasilAlternatif.forEach(item => { %>
      <tr>
        <td><%= item.alternatif %></td>
        <td><%= item.nilai.toFixed(4) %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('partials/footer') %>
